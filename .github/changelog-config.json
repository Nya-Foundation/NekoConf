{
    "categories": [
        {
            "title": "## 🚀 Features",
            "labels": [
                "feature",
                "enhancement"
            ]
        },
        {
            "title": "## 🐛 Bug Fixes",
            "labels": [
                "fix",
                "bug"
            ]
        },
        {
            "title": "## 📝 Documentation",
            "labels": [
                "documentation"
            ]
        },
        {
            "title": "## 🧰 Maintenance",
            "labels": [
                "chore",
                "dependencies"
            ]
        }
    ],
    "template": "${{CHANGELOG}}\n\n## 📦 Other Changes\n\n${{UNCATEGORIZED}}",
    "pr_template": "- ${{TITLE}} (#${{NUMBER}})",
    "empty_template": "No changes in this release",
    "transformers": [
        {
            "pattern": "[Bb]reaking( change)?",
            "target": "## ⚠️ BREAKING CHANGES\n"
        }
    ],
    "max_tags_to_fetch": 200,
    "max_pull_requests": 200,
    "max_back_track_time_days": 90
}